public class nineandnine {

	
	public static void main(String[] args) {

		// TODO Auto-generated method stub

		for (int i = 2; i <= 9; i++) {
			for (int j = 1; j <= 9; j++) {
				System.out.print(i + "*" + j + "=" + i * j + " ");
				if (j == 9) {
					System.out.println("");
				}
			}

		}
		System.out.println("");

	}

}
